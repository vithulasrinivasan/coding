#include<stdio.h>
int main()
{
    int num;
    for(num=1;num<=10;++num)
    {
        printf("\n%d",num);
    }
}


or


#include<stdio.h>
int main()
{
    int i=1;
    while(i<=10)
    {
        printf("\n%d",i);
        i++;
    }
}

or

#include<stdio.h>
int main()
{
    int i=1;
    do
    {
        printf("\n%d",i);
        i++;
    }while(i<=10);
}
