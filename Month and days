#include<stdio.h>
int main()
{
 int month; 
 printf("enter the month number");
 scanf("%d",&month);
 if(month==1)
 printf("\n31");
 else if(month==2)
 printf("\n28 or 29");
 else if(month==3)
 printf("\n31");
 else if(month==4)
 printf("\n30");
 else if(month==5)
 printf("\n31");
 else if(month==6)
 printf("\n30");
 else if(month==7)
 printf("\n31");
 else if(month==8)
 printf("\n31");
 else if(month==9)
 printf("\n30");
 else if(month==10)
 printf("\n31");
 else if(month==11)
 printf("\n30");
 else if(month==12)
 printf("\n31");
 else
 printf("\ninvalid day");
 return 0;
 }
 
