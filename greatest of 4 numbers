#include<stdio.h>
int main()
{
    int a,b,c,d;
    printf("enter the values for a,b,c and d:");
    scanf("%d%d%d%d",&a,&b,&c,&d);
    if(a>b && a>c && a>d)
    printf("\na is the greatest");
    else if(b>c && b>d)
    printf("\nb is the greatest");
    else if(c>d)
    printf("\nc is the greatest");
    else if(a==b && b==c && c==d)
    printf("\nall are equal");
    else
    printf("\nd is the greatest");
    return 0;
}
